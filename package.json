{
  "name": "@ethda/blobs",
  "version": "0.9.0",
  "description": "Ethda Blobs SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --no-cache",
    "lint": "tsdx lint",
    "fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:crustio/ethda-blobs.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "np": {
    "anyBranch": false,
    "branch": "main",
    "cleanup": true,
    "tests": false,
    "yarn": true,
    "contents": "."
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/blobs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/blobs.esm.js",
      "limit": "10 KB"
    }
  ],
  "author": "crustbuild",
  "license": "ISC",
  "dependencies": {
    "ethers": "^5.6.0",
    "ethjs-util": "^0.1.3",
    "keccak": "^1.0.2",
    "lodash": "^4.17.21",
    "utf-8-validate": "^5.0.9"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^14.11.2",
    "dotenv": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "np": "^7.6.1",
    "size-limit": "^7.0.8",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^7.0.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
